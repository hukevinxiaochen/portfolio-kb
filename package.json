{
  "name": "personal",
  "version": "1.0.0",
  "description": "a personal website with powerful note sharing and search options",
  "main": "server",
  "scripts": {
    "start": "node -r '@babel/register' server",
    "test": "TEST=true tape -r '@babel/register' './tests/**/*.test.js' | tap-spec",
    "build-html": "nodemon -r '@babel/register' b3d",
    "build-js-dev": "webpack --config webpack.dev.config.js"
  },
  "nodemonConfig": {
    "ext": "mdx,js,jsx",
    "ignore": ["tests/*"],
    "delay": "2500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khu-md/personal.git"
  },
  "author": "Kevin Hu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khu-md/personal/issues"
  },
  "homepage": "https://github.com/khu-md/personal#readme",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@ckeditor/ckeditor5-build-inline": "^23.1.0",
    "@ckeditor/ckeditor5-react": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.2",
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "feather-icons": "^4.28.0",
    "neo4j-driver": "^4.1.2",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "swipejs": "^2.2.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "jsdom": "^16.4.0",
    "mock-fs": "^4.13.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  }
}
